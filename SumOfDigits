package week1.day2;

public class SumOfDigits {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int input=123;
		int sum=0;
		int remainder=0;
		while(input>0)
		{			
			remainder= input%10;		
			sum=sum+remainder;		
			input= input/10;												
		}
		System.out.println(sum);	
	}

}
