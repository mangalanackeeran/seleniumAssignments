package week1.day1;

public class PrimeNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int input = 13;
		boolean flag = false;
		int remainder;

		for (int i = 2; i < input / 2;) {
			remainder = input % i;
			if (remainder == 1) {
				flag = true;
			}
			break;
		}

		if (flag) {
			System.out.println("Prime");

		} else {
			System.out.println("Not a Prime");
		}

	}

}
